{
  "name": "postman-newman-jenkins-allure",
  "version": "1.0.0",
  "description": "Project for newman tests",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "api-test": "newman run tests/testCollection.postman_collection.json  --environment tests/testMyEnvironment.postman_environment.json --reporters=cli,junit,html,htmlextra,allure --reporter-htmlextra-export 'newman/result.html' --reporter-junit-export 'newman/report.xml' --reporter-html-export 'newman/report.html' --reporter-allure-export 'tests'"
  },
  "author": "Davletova Rufina",
  "dependencies": {
    "newman": "^5.3.0",
    "newman-reporter-allure": "^1.0.7",
    "newman-reporter-htmlextra": "^1.22.1"
  }
}
